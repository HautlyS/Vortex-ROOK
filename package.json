{
  "name": "rook",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "tauri": "tauri",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix",
    "format": "prettier --write src/",
    "wasm:build": "cd src-wasm && wasm-pack build --target web --out-dir pkg",
    "wasm:dev": "cd src-wasm && wasm-pack build --target web --out-dir pkg --dev",
    "web:dev": "pnpm wasm:dev && BUILD_TARGET=web vite",
    "web:build": "pnpm wasm:build && BUILD_TARGET=web vue-tsc --noEmit && BUILD_TARGET=web vite build && cp src-wasm/pkg/*.wasm dist-web/",
    "web:preview": "BUILD_TARGET=web vite preview --outDir dist-web",
    "web:landing": "pnpm wasm:dev && BUILD_TARGET=web VITE_LANDING=true vite",
    "web:build:landing": "pnpm wasm:build && BUILD_TARGET=web VITE_LANDING=true vue-tsc --noEmit && BUILD_TARGET=web VITE_LANDING=true vite build && cp src-wasm/pkg/*.wasm dist-web/"
  },
  "dependencies": {
    "@tauri-apps/api": "^2.0.0",
    "@tauri-apps/plugin-dialog": "^2.0.0",
    "@tauri-apps/plugin-fs": "^2.0.0",
    "fabric": "^6.4.0",
    "pdfjs-dist": "^5.4.530",
    "pinia": "^2.2.0",
    "tesseract.js": "^5.1.1",
    "vue": "^3.5.0"
  },
  "devDependencies": {
    "@tauri-apps/cli": "^2.0.0",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/vue": "^8.1.0",
    "@types/jest-axe": "^3.5.9",
    "@types/jszip": "^3.4.1",
    "@types/node": "^22.0.0",
    "@vitejs/plugin-vue": "^5.2.0",
    "@vitest/coverage-v8": "^3.0.0",
    "@vue/tsconfig": "^0.7.0",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.15.0",
    "eslint-plugin-vue": "^9.31.0",
    "fast-check": "^3.22.0",
    "gsap": "^3.14.2",
    "jest-axe": "^10.0.0",
    "jsdom": "^25.0.0",
    "jszip": "^3.10.1",
    "mathjs": "^15.1.0",
    "motion-v": "^1.7.5",
    "postcss": "^8.4.49",
    "prettier": "^3.4.0",
    "tailwindcss": "^3.4.15",
    "typescript": "^5.6.0",
    "typescript-eslint": "^8.15.0",
    "vite": "^6.0.0",
    "vitest": "^3.0.0",
    "vue-tsc": "^2.1.0"
  }
}
